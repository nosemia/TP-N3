#include <iostream>
using namespace std;
int main() {
 int n, valor, pares = 0, impares = 0;
 cout << "Ingrese la cantidad de valores: ";
 cin >> n;
 for (int i = 0; i < n; i++) {
 cout << "Ingrese valor: ";
 cin >> valor;
 if (valor % 2 == 0)
 pares++;
 else
 impares++;
 }
 cout << "Pares: " << pares << ", Impares: " << impares << endl;
 return 0;
}
